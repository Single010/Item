
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>快医预约</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <style type="text/css">
        * {
            margin: 0px;
            padding: 0px;
        }

        .gh-nav {
            width: auto;
            border: 1px solid #F1ECEC;
            background-color: #F5F5F5;
        }

        .container {
            width: 80%;
            height: 30px;
            line-height: 30px;
            text-align: center;
            margin: 0 auto;
            display: flex;
            flex-flow: row nowrap;
            font-size: 12px;
        }

        .g-left {
            flex-grow: 1;
            flex-shrink: 1;
            /*border: 1px solid black;*/
        }

        .g-right {
            margin-left: 400px;
            flex-grow: 3;
            flex-shrink: 1;
            /*border:1px solid black;*/
        }

            .g-left a, .g-right a {
                margin-left: 10px;
                margin-right: 10px;
                color: black;
                text-decoration: none;
            }

        .gh-main {
            width: auto;
            height: 122px;
            /*border: 1px solid red;*/
        }

        .g-clear1 {
            width: 80%;
            height: 122px;
            line-height: 122px;
            /*border: 1px solid red;*/
            display: flex;
            flex-flow: row nowrap;
        }


        .gh-search {
            border: 3px solid #6EB92C;
            width: 497px;
            height: 40px;
            line-height: 40px;
            margin-top: 40px;
            margin-left: 150px;
        }

        .gh-search-tips {
            flex-shrink: 1;
            flex-grow: 3;
        }

        .gh-search-form {
            display: flex;
            flex-flow: row nowrap;
        }

        /*导航*/
        .navigation {
            width: auto;
            height: 37px;
            /*border:1px solid red;*/
        }

            .navigation .home-menu-ctn {
                /*z-index:2;*/
                width: 80%;
                height: 36px;
                /*border:1px solid red;*/
                margin: 0 auto;
            }

                .navigation .home-menu-ctn ul {
                    width: 80%;
                    height: 34px;
                    line-height: 34px;
                    list-style: none;
                    display: flex;
                    flex-flow: row nowrap;
                }

                    .navigation .home-menu-ctn ul li {
                        text-align: center;
                        flex-shrink: 1;
                        flex-grow: 1;
                    }

                        .navigation .home-menu-ctn ul li a {
                            text-decoration: none;
                            color: black;
                            font-size: 18px;
                        }

                            .navigation .home-menu-ctn ul li a:hover {
                                color: #2B73CC;
                            }
        /*分割线*/
        .fg {
            margin-top: -5px;
            width: auto;
            height: 3px;
            background-color: #2B73CC;
        }

        /*轮播*/
        .blue {
            width: auto;
            height: 350px;
            /*border: 1px solid red;*/
            background-color: #D2E5F6;
        }

            .blue .percent {
                width: 80%;
                height: 348px;
                /*border: 1px solid red;*/
                margin: 0 auto;
                display: flex;
                flex-flow: row nowrap;
            }

        .ad, .Carousel, .case {
            flex-shrink: 1;
            flex-grow: 1;
        }

        .blue .percent li {
            list-style: none;
            color: white;
        }

            .blue .percent li a {
                color: white;
            }

        .main-menu-nav1 li {
            width: 195px;
            text-align: center;
            background-color: #377BEE;
        }

            .main-menu-nav1 li:hover {
                background-color: white;
                color: #377BEE;
                border-bottom: 1px solid #377BEE;
            }

                .main-menu-nav1 li:hover a {
                    color: #377BEE;
                }

                .main-menu-nav1 li:hover li {
                    background-color: white;
                }

        .main-menu-nav1 .main {
            height: 65px;
            border-bottom: 1px solid white;
        }

        .main-menu-nav1 li .assistant {
            display: flex;
            flex-flow: row nowrap;
            justify-content: center;
        }

            .main-menu-nav1 li .assistant span {
                margin: 0 5px;
            }

        .ad {
            width: 209px;
            height: 350px;
            /*border:1px solid red;*/
        }
    </style>
</head>
<body style="margin:0px;padding:0px;">
    <div class="gh-nav">
        <div class="container g-clear">
            <div class="g-left" id="">
                <span class="text">您好！ 请</span>
                <span class="login"></span>
                <a href="">登录</a>
                <span class="line">|</span>
                <a href="">注册</a>
                <a href=""><i></i>我是医生</a>
            </div>


            <div class="g-right">

                <a href="text">
                    下载APP
                    <span>
                        <img src="https://static.guahao.cn/front/portal-pc-static/img/index-appQRcode.png?v=20170505" alt="" style="display: none;">
                    </span>
                </a>
                <span class="line">|</span>
                <a href="">快速取消预约</a>
                <span class="line">|</span>
                <a href="">客服中心</a>
                <span class="line">|</span>
                <a href="">微医云</a>
                <span class="line">|</span>
                <a href="">药品信息</a>
            </div>
        </div>
    </div>


    <div class="gh-main">
        <div class="container g-clear1" style="width: 1230px">
            <div class="gh-logo ">
                <img src="~/Content/Images/标志.jpg" alt="微医" class="wy-logo" />
            </div>
            <div class="gh-search gh-web-search gh-search-left ">
                <form class="gh-search-form" action="" name="qform">
                    <span class="glyphicon glyphicon-search" style=" width: 90%; height: 30px; line-height: 30px;">
                        <span class="search-icon"></span>
                        <input type="text" name="q" value="" autocomplete="off" maxlength="50" class="J_SearchQ" placeholder="请输入医院名" style="width:90%; border:none;outline:none" />
                    </span>
                    <input type="submit" value="搜索" class="gbn gbt-green2" style="height:34px;width:50px;background-color:#6EB92C;border:none;outline:none;color:white;font-size:16px;" />
                    <div class="clear"></div>
                </form>
            </div>
            <span class="gh-search-tips">
                <img src="~/Content/Images/品台.jpg" />
            </span>
        </div>
    </div>

    <div class="navigation">
        <div class="home-menu-ctn">
            <ul class="main-menu-nav">
                <li style="background-color:#2B73CC; width:73px;"><a href="" style="color:white;">帮你快速找医生</a></li>
                <li><a href="">我的预约</a></li>
                <li><a href="">我的评论</a></li>
                <li><a href="">我的问诊</a></li>
                <li><a href="">我的信息</a></li>
            </ul>
        </div>
    </div>
    @*分割线*@
    <div class="fg"></div>

    @*轮播*@
    <div class="blue">
        <div class="percent">
            <div class="case">
                <ul class="main-menu-nav1">
                    <li class="main"><span>内科</span><ul class="assistant"><li><a href="">糖尿病</a></li><span>|</span><li><a href="">高血压</a></li><span>|</span><li><a href="">冠心病</a></li></ul></li>
                    <li class="main"><span>内科</span><ul class="assistant"><li><a href="">糖尿病</a></li><span>|</span><li><a href="">高血压</a></li><span>|</span><li><a href="">冠心病</a></li></ul></li>
                    <li class="main"><span>内科</span><ul class="assistant"><li><a href="">糖尿病</a></li><span>|</span><li><a href="">高血压</a></li><span>|</span><li><a href="">冠心病</a></li></ul></li>
                    <li class="main"><span>内科</span><ul class="assistant"><li><a href="">糖尿病</a></li><span>|</span><li><a href="">高血压</a></li><span>|</span><li><a href="">冠心病</a></li></ul></li>
                    <li class="main"><span>内科</span><ul class="assistant"><li><a href="">糖尿病</a></li><span>|</span><li><a href="">高血压</a></li><span>|</span><li><a href="">冠心病</a></li></ul></li>
                    <li class="main"><span class="glyphicon glyphicon-plus"></span><span>更多科室</span></li>
                </ul>
            </div>

            <div class="Carousel" style="width:800px;height:350px;@*border:1px solid red*@;margin-left:-13px;">

                <!--轮播图-->
                <div class="container">
                    <div class="carousel slide" id="slidershow" data-ride="carousel" data-interval="1500">

                        <!--图片容器-->
                        <div class="carousel-inner" style="width:810px;height:350px;margin-left:-95px;">
                            <div class="item active">
                                <img src="~/Content/Images/医院1.jpg" style="height:350px;width:810px;" />
                                <!--添加对应标题和内容-->
                                <div class="carousel-caption">
                                    <h4>图一</h4>
                                    <span>内容</span>
                                </div>
                            </div>
                            <div class="item">
                                <img src="~/Content/Images/医院2.jpg" style="height:350px;width:810px;" />
                                <!--添加对应标题和内容-->
                                <div class="carousel-caption">
                                    <h4>图二</h4>
                                </div>
                            </div>
                            <div class="item">
                                <img src="~/Content/Images/医院3.jpg" style="height:350px;width:810px;" />
                                <!--添加对应标题和内容-->
                                <div class="carousel-caption">
                                    <h4>图三</h4>
                                </div>
                            </div>
                            <div class="item">
                                <img src="~/Content/Images/医院4.jpg" style="height:350px;width:810px;" />
                                <!--添加对应标题和内容-->
                                <div class="carousel-caption">
                                    <h4>图四</h4>
                                </div>
                            </div>
                            <!--计数器-->
                            <ol class="carousel-indicators">
                                <li class="active" data-target="#slidershow" data-slide-to="0"></li>
                                <li data-target="#slidershow" data-slide-to="1"></li>
                                <li data-target="#slidershow" data-slide-to="2"></li>
                                <li data-target="#slidershow" data-slide-to="3"></li>
                            </ol>
                            <!--轮播导航-->
                            <a href="#slidershow" data-slide="prev" class="left carousel-control" role="button">
                                @*左滑按钮*@
                                <span class="glyphicon glyphicon-chevron-left"></span>
                            </a>
                            <a href="#slidershow" data-slide="next" class="right carousel-control" role="button">
                                @*右滑按钮*@
                                <span class="glyphicon glyphicon-chevron-right"></span>
                            </a>
                        </div>
                    </div>
                </div>

            </div>

            <div class="ad">
                <img src="~/Content/Images/one.png" />
                <img src="~/Content/Images/three.png" />
                <img src="~/Content/Images/two.png" />
            </div>
        </div>
    </div>


</body>
</html>
